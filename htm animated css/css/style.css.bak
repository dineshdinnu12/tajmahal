*{ margin:0; padding:0; box-sizing: border-box;

}

header
{
	width:100%;
	height:100vh;
	background-image:linear-gradient(rgba(0,0,0,0.1),rgba(0,0,0,0.1)),url('../ui.jpeg');
	background-repeat:no-repeat;
	background-position:center;
	background-size:cover;
}
nav
{
	width:100%;
	height:10vh;
	background-color:rgba(0,0,0,0.2);
	color:white; display:flex; justify-content: space-between;
	align-items:center; 
	text-transform:uppercase;
	
}
.logo{
	width:25%; text-align:cent;
	
}
.menu
{ width:40%;
display:flex; justify-content:space-around;
}
.menu a
{ color:white;
font-weight:bold;
transition:0.5s ease;

}
.menu a:hover{background-color:white;
color:black;
padding:7px;
border-radius:4px;
}
a.id
{ background-color:white;
color:black;
padding:7px;
border-radius:4px;
}
main
{
	width:100%;
	height:90%;

	display:flex; justify-content:center;
	align-items:center;
	text-align:center;
	color:white;
}
section{
}

h3
{font-size:35px;
font-weight:200;
letter-spacing:3px;
	
}
section h1
{
	margin:30px 0 20px 0;
	font-size: 55px;
	text-transform:uppercase;
	
}
section p
{
	font-size:25px;
	color:;
	margin-bottom:25px;
}
section a
{
	padding:12px 30px;
	border-radius:4px;
	outline:none;
	font-size:13px;
	text-transform:uppercase;
	font-weight:500;
	text-decoration:none;
}
.btnone
{background-color:#fff;
color:#000;

}
.btntwo
{
	background-color:#00a891;
color:#000;
}

.change_content:after{
	content:'';
	animation:changetext 10s infinite linear;
	color:#00a891;
}

@keyframes changetext{
	0%{content:"Darjeeling";}
	20%{content:"Agra";}
	35%{content:"jaipur";}
	60%{content:"kerala";}
	80%{content:"delhi";}
	100%{content:"mumbai";}
}